# 所有主机
[all-server]
192.168.0.183 host_name=k8s-master01
192.168.0.184 host_name=k8s-worker01



# 安装harbor主机
[harbor]
192.168.0.183

# 安装k8s主机,gpu=1为有gpu服务器,gpu=0为无gpu服务器
[k8s]
192.168.0.183 k8s_role=master gpu=1
192.168.0.184 k8s_role=worker gpu=0

[all:vars]
ansible_ssh_port=22
ansible_ssh_user=root
ansible_ssh_pass=newerabc@123

# docker安装目录
docker_home_dir=/data/docker

# harbor安装目录
harbor_home=/data/harbor
# harbor地址
harbor_host=192.168.0.183
# harbor管理员密码
harbor_admin_password=Harbor12345

# harbor端口
harbor_port=8888
